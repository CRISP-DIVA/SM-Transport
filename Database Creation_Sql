CREATE Table users(user_id INT NOT NULL AUTO_INCREMENT,name VARCHAR(30) NOT NULL,email VARCHAR(50) NOT NULL,PRIMARY KEY (user_id));

CREATE TABLE rutas(ruta_id INT NOT NULL AUTO_INCREMENT,originlat FLOAT NOT NULL,originlng FLOAT NOT NULL,destlat FLOAT NOT NULL,destlng FLOAT NOT NULL,hora_sortida DATETIME NOT NULL,trans_mode VARCHAR(30) NOT NULL,user_id INT NOT NULL,CONSTRAINT FK_UserId FOREIGN KEY (user_id) REFERENCES users (user_id),PRIMARY KEY (ruta_id));

CREATE TABLE tram(tram_id INT NOT NULL AUTO_INCREMENT,lat FLOAT NOT NULL,lng FLOAT NOT NULL,PRIMARY KEY(tram_id));

CREATE TABLE hores_tram(hores_id INT NOT NULL AUTO_INCREMENT,hora DATETIME NOT NULL,densitat FLOAT NOT NULL,tram_id INT NOT NULL,CONSTRAINT FK_tramId FOREIGN KEY (tram_id) REFERENCES tram (tram_id),PRIMARY KEY (hores_id));